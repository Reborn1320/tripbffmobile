    "i18next": "11.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "native-base": "2.12.1",
    "react": "16.8.6",
    "react-i18next": "9.0.2",
    "react-native": "0.60.4",
    "react-native-action-button": "^2.8.5",
    "react-native-autocomplete-input": "^4.1.0",
    "react-native-calendar-picker": "^6.0.0",
    "react-native-camera-roll-picker": "^1.3.0",
    "react-native-elements": "1.1.0",
    "react-native-fast-image": "^7.0.2",
